{
    "version": 2,
    "builds": [
      {
        "src": "../client/dist",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "rewrites": [
      {
          "source": "/api/(.*)",
          "destination": "/api/index.js"
      }
    ]
  }
  