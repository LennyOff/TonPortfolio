{
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" },
      { "src": "client/dist/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "https://ton-portfolio-server.vercel.app/:1" },
      { "src": "/client/(.*)", "dest": "/client/$1" },
      { "src": "/(.*)", "dest": "/index.js" }
    ]
  }